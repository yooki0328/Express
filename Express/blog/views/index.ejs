<%include ./base/head.ejs%>

<div class="h1">Home</div>
<div class="col-sm-10 article-list">
<%if(data){%>
<%data.forEach(function(art,index){%>

<div class="article">
<span class="h3 text-info"><%=art.title%></span>

<div class="art_info">author:<%=art.author%>|date:<%=art.time%></div>
<div class="content">
	<%=art.content%>
</div>
</div>
<%})%>
<%}%>	

<%include ./base/foot.ejs%>

